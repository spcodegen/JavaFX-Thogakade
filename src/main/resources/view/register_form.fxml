            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="264.0" layoutY="112.0" promptText="User Name" />
      <TextField fx:id="txtEmail" layoutX="264.0" layoutY="152.0" promptText="Email" />
      <TextField fx:id="txtPassword" layoutX="264.0" layoutY="191.0" promptText="Password" />
      <TextField fx:id="txtCPassword" layoutX="264.0" layoutY="229.0" promptText="Confirm Password" />
